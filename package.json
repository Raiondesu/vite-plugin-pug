{
  "name": "vite-plugin-pug",
  "version": "0.2.0",
  "description": "Vite plugin for transform Pug templates into HTML",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node --experimental-json-modules scripts/watcher.js",
    "typecheck": "tsc --noEmit",
    "forest": "node --loader ts-node/esm.mjs scripts/forest.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "watch": "node --experimental-json-modules scripts/watcher.js",
    "wipe": "bash scripts/wipe.sh",
    "test": "cross-env NODE_OPTIONS=--experimental-json-modules npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SubZtep/vite-plugin-pug.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "pug",
    "jade"
  ],
  "author": "Andras Serfozo <subztep@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/SubZtep/vite-plugin-pug/issues"
  },
  "homepage": "https://github.com/SubZtep/vite-plugin-pug#readme",
  "dependencies": {
    "@dovyih/x-tree-diff-plus": "^0.1.4",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/pug": "^2.0.4",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "vite": "~2"
  }
}
